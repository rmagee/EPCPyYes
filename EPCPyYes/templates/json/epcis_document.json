{"EPCISDocument":{
"creationDate":"{{ created_date }}",
{% block header %}
{% if header %}
"EPCISHeader":{
{# include "json/sbdh.json" ***************************** #}
}
{% endif %}
{% endblock %}
"EPCISBody":
{
"EventList":
[
{% block object_events %}
{% if object_events|length > 0 %}
{% for event in object_events %}
{% include "json/object_event.json" %}{{"," if not loop.last or aggregation_events|length > 0 or transaction_events|length > 0 or transformation_events|length > 0 }}
{% endfor %}
{% endif %}
{% endblock %}
{% block aggregation_events %}
{% if aggregation_events|length > 0 %}
{% for event in aggregation_events %}
{% include "json/aggregation_event.json" %}{{"," if not loop.last or transaction_events|length > 0 or transformation_events|length > 0 }}
{% endfor %}
{% endif %}
{% endblock %}
{% block transaction_events %}
{% if transaction_events|length > 0 %}
{% for event in transaction_events %}
{% include "json/transaction_event.json" %}{{"," if not loop.last or transformation_events|length > 0 }}
{% endfor %}
{% endif %}
{% endblock %}
{% block transformation_events %}
{% if transformation_events|length > 0 %}
{% for event in transformation_events %}
{% include "json/transformation_event.json" %}{{"," if not loop.last }}
{% endfor %}
{% endif %}
{% endblock %}
]
}
}
}

