{% if event.event_id or event.error_declaration %}
"baseExtension": {
{% if event.event_id %}
"eventID":"{{ event.event_id }}"{{"," if event.error_declaration }}
{% endif %}
{% if event.error_declaration %}
  "errorDeclaration":{
  "declarationTime":"{{ event.error_declaration.declaration_time }}",
  "reason":"{{ event.error_declaration.reason }}"{{"," if event.error_declaration.corrective_event_ids|length > 0 }}
  "correctiveEventIDs":[
  {% for eventid in event.error_declaration.corrective_event_ids %}
  "{{ eventid }}"{{"," if not loop.last}}
  {% endfor %}
  ]
  }
},
{% endif %}
{% endif %}